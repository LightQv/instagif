{
  "version": 2,
  "builds": [{ "src": "./index.js", "use": "@vercel/node" }],
  "rewrites": [
    { "source": "/api/", "destination": "https://insta-gif.vercel.com/api" },
    {
      "source": "/api/:match*",
      "destination": "https://insta-gif.com/api/:match*"
    },
    { "source": "/", "destination": "https://insta-gif.vercel.com/" },
    {
      "source": "/:match*",
      "destination": "https://insta-gif.vercel.com/:match*"
    }
  ]
}
